[test_case]
test = perfrunner.tests.kv.MixedLatencyTest
title = KV latency (ms), 1 bucket x 100M x 1KB, DGM 05pct, 1875 mutations/sec/node
summary = Read-heavy query test (80/20), 4 nodes, 1 bucket x 100M x 1KB, DGM 05pct, 1875 mutations/sec/node, unbounded qps
larger_is_better = true

[cluster]
mem_quota = 12000
initial_nodes = 4
num_buckets = 1

[bucket]
replica_number = 1
eviction_policy = fullEviction

[compaction]
db_percentage = 80

[load]
items = 10000000
size = 1024
workers = 20

[access]
creates = 0
reads = 50
updates = 50
deletes = 0
throughput = 15000
items = 10000000
workers = 12
time = 3600
