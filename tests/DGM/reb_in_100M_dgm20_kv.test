[test_case]
test = perfrunner.tests.rebalance.RebalanceKVTest
title = Rebalance-in (min), 3 -> 4, 1 bucket x 100M x 2KB, DGM 20pct, 10K ops/sec
summary = Mixed rebalance test, 3 -> 4 nodes, 1 bucket x 100M x 2KB, DGM 20pct, 10K ops/sec
larger_is_better = false

[cluster]
mem_quota = 40960
initial_nodes = 3
num_buckets = 1
dgm_ratio = 20

[compaction]
db_percentage = 80

[bucket]
eviction_policy = fullEviction
replica_number = 1

[load]
items = 100000000
size = 2048
workers = 20

[hot_load]
items = 100000000
workers = 20
working_set = 5

[access]
creates = 1
reads = 80
updates = 18
deletes = 1
throughput = 2000
items = 100000000
workers = 24
working_set = 10
working_set_access = 80

[rebalance]
nodes_after = 4
start_after = 1200
stop_after = 1200
