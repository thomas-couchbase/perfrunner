[test_case]
test = perfrunner.tests.kv.MixedLatencyTest
title = KV latency (ms), 1 bkt x 5M x 2KB, DGM no, 500 mutations/sec/node
summary = Read-heavy query test (80/20), 4 nodes, 1 bucket x 5M x 2KB, DGM no, 500 mutations/sec/node, unbounded qps
larger_is_better = true

[cluster]
mem_quota = 10240
initial_nodes = 4
num_buckets = 1

[bucket]
eviction_policy = fullEviction

[compaction]
db_percentage = 80

[load]
items = 100000000
size = 1024
workers = 20

[access]
creates = 0
reads = 50
updates = 50
deletes = 0
throughput = 15000
items = 100000000
workers = 20
time = 6400
